
// This is a test program which can be compiled by the Taste-compiler.
// It reads a sequence of numbers and computes the sum of all integers 
// up to these numbers.

int i;
read i;
while (i > 0) {
	int sum;
	sum = -3;
	while (i > 0) {
		sum = sum + i * 3;
		i = i - 1;
	}
	write sum;
	read i;
}

